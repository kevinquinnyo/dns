{
    "name": "kevinquinnyo/dns",
    "description": "Dependency-free PHP DNS resolution library",
    "type": "library",
    "authors": [
        {
            "name": "Kevin Quinn",
            "email": "kevin@kevops.com"
        }
    ],
    "require": {
        "php": ">=7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "kevinquinnyo\\DNS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kevinquinnyo\\Test\\": "tests/"
        }
    }
}
